<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sinemmm">
    <View
        android:id="@+id/view2"
        android:layout_width="418dp"
        android:layout_height="100dp"
        android:background="@color/bizim"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="109dp"
        android:layout_height="100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo2" />
    <ImageButton
        android:id="@+id/paylas"
        android:layout_width="50dp"
        android:layout_height="73dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="5dp"
        android:layout_toStartOf="@+id/profil"
        android:backgroundTint="#2A484D"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toStartOf="@+id/profil"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/paylas" />

    <ImageButton
        android:id="@+id/profil"
        android:layout_width="63dp"
        android:layout_height="97dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="5dp"
        android:backgroundTint="#2A484D"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profil" />
    <ImageButton
        android:id="@+id/pusula1"
        android:layout_width="50dp"
        android:layout_height="72dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="5dp"
        android:layout_toStartOf="@+id/paylas"
        android:backgroundTint="#2A484D"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toStartOf="@+id/paylas"
        app:layout_constraintStart_toEndOf="@+id/NAUTILUS"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pusula" />

    <TextView
        android:id="@+id/NAUTILUS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="5dp"
        android:layout_toStartOf="@+id/pusula1"
        android:text="NAUTILUS"
        android:textColor="@color/nautilus"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toStartOf="@+id/pusula1"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="502dp"
        android:layout_below="@+id/view2"
        android:layout_marginTop="-8dp"
        android:background="#F7F7F7"
        android:backgroundTint="#80F7F7F7"
        android:foregroundTint="#80FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:srcCompat="@drawable/akdeniziki" />

    <Button
        android:id="@+id/antalyabir"
        android:layout_width="200dp"
        android:layout_height="125dp"
        android:layout_above="@+id/button4"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="-1dp"
        android:backgroundTint="#A627484C"
        android:text="ANTALYA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <Button
        android:id="@+id/biradana"
        android:layout_width="200dp"
        android:layout_height="125dp"
        android:layout_above="@+id/button4"
        android:layout_alignParentEnd="true"
        android:backgroundTint="#A627484C"
        android:text="ADANA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <Button
        android:id="@+id/button3"
        android:layout_width="200dp"
        android:layout_height="125dp"
        android:layout_above="@+id/button5"
        android:layout_alignParentEnd="true"
        android:backgroundTint="#A627484C"
        android:text="BURDUR"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/antalyabir" />

    <Button
        android:id="@+id/button4"
        android:layout_width="200dp"
        android:layout_height="125dp"
        android:layout_above="@+id/button5"
        android:backgroundTint="#A627484C"
        android:text="KONYA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <Button
        android:id="@+id/button5"
        android:layout_width="200dp"
        android:layout_height="125dp"
        android:layout_above="@+id/button7"
        android:layout_alignParentEnd="true"
        android:backgroundTint="#A61F494E"
        android:text="ISPARTA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

    <Button
        android:id="@+id/button6"
        android:layout_width="200dp"
        android:layout_height="125dp"
        android:layout_above="@+id/button8"
        android:layout_below="@+id/button4"
        android:layout_alignParentStart="true"
        android:backgroundTint="#A628484D"
        android:text="HATAY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/biradana" />

    <Button
        android:id="@+id/button7"
        android:layout_width="200dp"
        android:layout_height="125dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:backgroundTint="#A627484C"
        android:text="OSMANİYE"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button5" />

    <Button
        android:id="@+id/button8"
        android:layout_width="206dp"
        android:layout_height="123dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="0dp"
        android:backgroundTint="#A627484C"
        android:text="MERSİN"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button6" />


</RelativeLayout>