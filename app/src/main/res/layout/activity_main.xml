<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/imge"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAFAFA"
    tools:context=".MainActivity">

    <View
        android:id="@+id/view2"
        android:layout_width="418dp"
        android:layout_height="100dp"
        android:background="@color/bizim"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="109dp"
        android:layout_height="100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo2" />

    <ImageButton
        android:id="@+id/paylas"
        android:layout_width="50dp"
        android:layout_height="73dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="5dp"
        android:layout_toStartOf="@+id/profil"
        android:backgroundTint="#2A484D"
        android:scaleType="centerCrop"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/paylas" />

    <ImageButton
        android:id="@+id/profil"
        android:layout_width="63dp"
        android:layout_height="97dp"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="5dp"
        android:backgroundTint="#2A484D"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profil" />

    <ImageButton
        android:id="@+id/pusula1"
        android:layout_width="50dp"
        android:layout_height="72dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="5dp"
        android:layout_toStartOf="@+id/paylas"
        android:backgroundTint="#2A484D"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/pusula" />

    <ImageButton
        android:id="@+id/harita"
        android:layout_width="match_parent"
        android:layout_height="210dp"
        android:layout_below="@+id/view2"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-0dp"
        android:layout_marginEnd="0dp"
        android:backgroundTint="#FAFAFA"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/harita" />

    <TextView
        android:id="@+id/NAUTILUS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="5dp"
        android:layout_toStartOf="@+id/pusula1"
        android:text="NAUTILUS"
        android:textColor="@color/nautilus"
        android:textSize="25dp" />

    <ImageButton
        android:id="@+id/ak"
        android:layout_width="204dp"
        android:layout_height="72dp"
        android:layout_below="@+id/harita"
        android:layout_marginTop="0dp"
        android:backgroundTint="#FAFAFA"
        android:scaleType="centerCrop"


        app:srcCompat="@drawable/sinem" />

    <ImageButton
        android:id="@+id/karedeniz"
        android:layout_width="209dp"
        android:layout_height="72dp"
        android:layout_below="@+id/harita"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:layout_toEndOf="@+id/ak"
        android:backgroundTint="#FAFAFA"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/karadeniz" />

    <ImageButton
        android:id="@+id/ege"
        android:layout_width="204dp"
        android:layout_height="72dp"
        android:layout_below="@+id/ak"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:backgroundTint="#FAFAFA"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/ege" />

    <ImageButton
        android:id="@+id/icanadolu"
        android:layout_width="207dp"
        android:layout_height="72dp"
        android:layout_below="@+id/karedeniz"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:layout_toEndOf="@+id/ege"
        android:backgroundTint="#FAFAFA"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/icanadoglu" />

    <ImageButton
        android:id="@+id/güneyanadolu"
        android:layout_width="204dp"
        android:layout_height="70dp"
        android:layout_below="@+id/ege"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:backgroundTint="#FAFAFA"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/ganadolu" />

    <ImageButton
        android:id="@+id/doguanadolu"
        android:layout_width="244dp"
        android:layout_height="72dp"
        android:layout_below="@+id/icanadolu"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:layout_toEndOf="@+id/güneyanadolu"
        android:backgroundTint="#FAFAFA"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/dogu" />

    <ImageButton
        android:id="@+id/marmara"
        android:layout_width="204dp"
        android:layout_height="72dp"
        android:layout_below="@+id/güneyanadolu"
        android:layout_alignParentStart="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:backgroundTint="#FAFAFA"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/marmara" />

    <ImageButton
        android:id="@+id/fovariler"
        android:layout_width="209dp"
        android:layout_height="76dp"
        android:layout_below="@+id/doguanadolu"
        android:layout_marginStart="-2dp"
        android:layout_marginTop="0dp"
        android:layout_toEndOf="@+id/marmara"
        android:backgroundTint="#FAFAFA"
        android:scaleType="fitStart"
        app:srcCompat="@drawable/fovari" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="82dp"
        android:layout_height="25dp"
        android:layout_alignTop="@+id/ak"
        android:layout_alignEnd="@+id/ak"
        android:layout_alignBottom="@+id/ak"
        android:layout_alignParentStart="true"
        android:layout_marginStart="57dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="65dp"
        android:layout_marginBottom="24dp"
        android:text="AKDENİZ"
        android:textColor="#000000"
        android:textColorHighlight="#0B0101"
        android:textColorHint="#000000"
        android:textColorLink="#0C0101"
        android:textSize="20sp"
        android:textStyle="bold|italic" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="109dp"
        android:layout_height="34dp"
        android:layout_alignTop="@+id/karedeniz"
        android:layout_alignBottom="@+id/karedeniz"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="46dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="14dp"
        android:layout_toEndOf="@+id/ak"
        android:text="KARADENİZ"
        android:textColor="#020000"
        android:textColorHighlight="#020000"
        android:textColorHint="#040000"
        android:textColorLink="#090000"
        android:textSize="20sp"
        android:textStyle="bold|italic" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ak"
        android:layout_alignEnd="@+id/ege"
        android:layout_alignBottom="@+id/ege"
        android:layout_alignParentStart="true"
        android:layout_marginStart="73dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="73dp"
        android:layout_marginBottom="27dp"
        android:text="EGE"
        android:textColor="#0B0606"
        android:textColorHighlight="#090404"
        android:textColorHint="#020000"
        android:textColorLink="#0B0000"
        android:textSize="20sp"
        android:textStyle="bold|italic" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="111dp"
        android:layout_height="26dp"
        android:layout_below="@+id/karedeniz"
        android:layout_alignBottom="@+id/icanadolu"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="23dp"
        android:layout_toEndOf="@+id/ege"
        android:text="ANADOLU"
        android:textColor="#090606"
        android:textColorHighlight="#040202"
        android:textColorHint="#090000"
        android:textColorLink="#090000"
        android:textSize="20sp"
        android:textStyle="bold|italic" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:layout_below="@+id/icanadolu"
        android:layout_alignBottom="@+id/doguanadolu"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="22dp"
        android:layout_toEndOf="@+id/güneyanadolu"
        android:text="DOĞUANADOLU"
        android:textColor="#070101"
        android:textColorHighlight="#100101"
        android:textColorHint="#090000"
        android:textColorLink="#0B0000"
        android:textSize="20sp"
        android:textStyle="bold|italic" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="166dp"
        android:layout_height="31dp"
        android:layout_below="@+id/ege"
        android:layout_alignEnd="@+id/güneyanadolu"
        android:layout_alignBottom="@+id/güneyanadolu"
        android:layout_alignParentStart="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="18dp"
        android:text="GÜNEYANADOLU"
        android:textColor="#090606"
        android:textColorHighlight="#040202"
        android:textColorHint="#040202"
        android:textColorLink="#070505"
        android:textSize="20sp"
        android:textStyle="bold|italic" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="114dp"
        android:layout_height="28dp"
        android:layout_below="@+id/güneyanadolu"
        android:layout_alignEnd="@+id/marmara"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="43dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="26dp"
        android:text="MARMARA"
        android:textColor="#090606"
        android:textColorHighlight="#0B0909"
        android:textColorHint="#050000"
        android:textColorLink="#070000"
        android:textSize="20sp"
        android:textStyle="bold|italic" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="104dp"
        android:layout_height="29dp"
        android:layout_below="@+id/doguanadolu"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="69dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="26dp"
        android:layout_toEndOf="@+id/marmara"
        android:text="FAVORİLER"
        android:textColor="#100F0F"
        android:textColorHighlight="#0B0000"
        android:textColorHint="#070000"
        android:textColorLink="#020000"
        android:textSize="20sp"
        android:textStyle="bold|italic" />


</RelativeLayout>